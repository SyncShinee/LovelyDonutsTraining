#include <iostream>
#include <cstdio>
#include <cstring>

using namespace std;

typedef long long ll;

const int maxn = 1e3 + 100;

ll a[maxn][maxn], sl[maxn][maxn], sr[maxn][maxn], w;

void solve() {
    memset(sl, 0, sizeof(sl));
    memset(sr, 0, sizeof(sr));
    for(int i = 1; i <= n; i++) {
	for(int j = 1; j <= n; j++) {
	    sl[i][j] = sl[i - 1][j - 1] + a[i][j];
	}
    }
    for(int i = 1; i <= n; i++) {
	for(int j = n; j >= 1; j--) {
	    sr[i][j] = sr[i - 1][j + 1] + a[i][j];
	}
    }
}

int main() {
    freopen("L.txt", "r", stdin);
    
    return 0;
}
